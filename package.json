{
	"name": "roapi.js",
	"version": "1.0.0",
	"description": "An object-oriented JS wrapper for the Roblox API",
	"main": "index.js",
	"module": "index.js",
	"types": "./typings/index.d.ts",
	"typings": "./typings/index.d.ts",
	"files": [
		"src",
		"typings"
	],
	"exports": {
		"./*": "./*",
		".": {
			"require": "./src/index.js",
			"import": "./src/index.mjs"
		}
	},
	"type": "module",
	"scripts": {
		"test": "echo \"Test NYI\"",
		"prepublishOnly": "npm run test && gen-esm-wrapper ./src/index.js ./src/index.mjs",
		"lint:typings": "tslint typings/index.d.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/roblox-js/roapi.js.git"
	},
	"keywords": [
		"roblox",
		"roapi"
	],
	"author": "roblox-js",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/roblox-js/roapi.js/issues"
	},
	"homepage": "https://github.com/roblox-js/roapi.js#readme",
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"eslint": "^8.2.0",
		"gen-esm-wrapper": "^1.1.3",
		"tar": "^6.1.11",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"node-fetch": "^3.1.1"
	}
}